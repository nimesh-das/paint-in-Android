<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:weightSum="1"
    android:orientation="vertical">


    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton"
        android:src="@drawable/pic0" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton2"
        android:src="@android:drawable/ic_delete" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton3"
        android:src="@drawable/pic14" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton4"
        android:src="@drawable/pic15" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton5"
        android:src="@drawable/pic13" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton6"
        android:src="@android:drawable/dialog_frame"
        android:contentDescription="Fill" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton7"
        android:src="@android:color/holo_red_dark" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton8"
        android:src="@android:color/holo_green_dark" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton9"
        android:src="@android:color/holo_orange_light" />

    <ImageButton
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/Custom"
        android:src="@android:id/custom"
        android:contentDescription="custom" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="45dp">


        <ImageButton
            android:layout_width="15dp"
            android:layout_height="45dp"
            android:id="@+id/imageButton11" />

        <ImageButton
            android:layout_width="15dp"
            android:layout_height="45dp"
            android:id="@+id/imageButton12" />

        <ImageButton
            android:layout_width="15dp"
            android:layout_height="45dp"
            android:id="@+id/imageButton13" />

        <ImageButton
            android:layout_width="15dp"
            android:layout_height="45dp"
            android:id="@+id/imageButton14" />

    </LinearLayout>


</LinearLayout>
